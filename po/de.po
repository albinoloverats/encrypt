msgid "  -i, --in       FILE        Input file\n"
msgstr "  -i, --in       DATEI       Eingabedatei\n"

msgid "  -o, --out      FILE        Output file\n"
msgstr "  -o, --out      DATEI       Ausgabedatei\n"

msgid "  -k, --keyfile  FILE        Key file (must come first if generating a key)\n"
msgstr "  -k, --keyfile  DATEI       Schlüssel-Datei (zuerst kommen müssen,\n                             wenn die einen Schlüssel)\n"

msgid "  -f, --passfile FILE        File to use as passphrase to generate key\n"
msgstr "  -f, --passfile DATEI       Datei als Passwort zu verwenden, um Schlüssel\n"

msgid "  -p, --password PASSWORD    Use given password to generate key\n"
msgstr "  -p, --password PASSWORT    Verwendung bestimmten Passwort zu generieren \n                      Schlüssel\n"

msgid "  -e, --encrypt  ALGORITHM   Algorithm to use for encryption\n"
msgstr "  -e, --encrypt  ALGORITHMUS Algorithmus für die Verschlüsselung\n"

msgid "  -d, --decrypt  ALGORITHM   Algorithm to use for decryption\n"
msgstr "  -d, --decrypt  ALGORITHMUS Algorithmus für die Entschlüsselung\n"

msgid "  -a, --about    ALGORITHM   Information about a particular algorithm\n"
msgstr "  -a, --about    ALGORITHMUS Informationen über enien Algorithmus\n"

msgid "  -g, --generate SIZE        Generate a key of specified size (in bits)\n"
msgstr "  -g, --generate GRÖßE       Schlüssel erzeugen von bestimmten Größe\n"

msgid "  -m, --modules              List all available algorithms\n"
msgstr "  -m, --modules              Liste aller verfügbaren Algorithmus\n"

msgid "If -i or -o are omitted then stdin/stdout are used.  Either -e or -d must be\n"
      "present if you intend to do something, as well as -k -f or -p. Using -g will\n"
      "generate a random key and echo it to stdout unless -g is preceded by -k; the\n"
      "key can be used later with the -k option. However -a -m -h -l -v may be used\n"
      "on their own or not at all.\n"
msgstr "Wenn -i oder -o entfallen dann stdin / stdout werden. Entweder -e oder -d\n"
       "muss vorhanden, wenn Sie die Absicht, etwas zu tun, sowie -k -f oder -p. Mit\n"
       "-g einen zufälligen Schlüssel und Echo an stdout, es sei denn, -g wird mit\n"
       "-k, die -Taste kann verwendet werden, später mit der -k Option. Allerdings\n"
       "-a -m -h -l -v verwendet werden über ihre eigenen oder gar nicht.\n"

msgid "%s\n  version : %s\n  built on: %s %s\n\n%s\nWebsite\n  %s\n\nContributors\n  %s\n\nCopyright\n  %s\n\nLicence\n%s"
